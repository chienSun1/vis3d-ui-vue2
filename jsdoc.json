{
    "source": {
        "include": ["./src/lib/map3d-resource"],
        "includePattern": ".+\\.js(doc|x)?$",
        "exclude": [
            "./apidoc/","./src/lib/map3d-resource/sensor",
            "./src/lib/map3d-resource/gadgets/core",
            "./src/lib/map3d-resource/gadgets/styles",
            "./src/lib/map3d-resource/gadgets/svgPaths",
            "./src/lib/map3d-resource/gadgets/viewModels"

        ]
    },
    "plugins": [],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "./apidoc/",
        "recurse": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        }
    }
}